{
  "manifest_version": 3,
  "name": "Metruyencv Utils",
  "version": "1.0",
  "description": "Metruyencv Utils",
  "permissions": [],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["index.js"]
  }],
  "web_accessible_resources": [{
    "resources": ["enable-copy.js"],
    "matches": ["<all_urls>"]
  }]
}